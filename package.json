{
  "name": "accounting-system-v1.0.2",
  "homepage": "https://imarksky95.github.io/accountingsystemv101/",
  "version": "1.0.0",
  "description": "A modern web-based Accounting System with a React frontend, Node.js/Express backend, and PostgreSQL database. The system supports authentication, user and role management, and a scalable structure for future modules.",
  "main": "index.js",
  "scripts": {
    "test": "jest --runInBand",
    "test:server": "jest --config=jest.server.config.js --runInBand"
  },
  "jest": {
    "roots": ["<rootDir>/tests", "<rootDir>/frontend/tests"],
    "testMatch": [
      "<rootDir>/tests/**/*.test.[jt]s?(x)",
      "<rootDir>/tests/**/*.spec.[jt]s?(x)",
      "<rootDir>/frontend/tests/**/*.test.[jt]s?(x)",
      "<rootDir>/frontend/tests/**/*.spec.[jt]s?(x)"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "axios": "^1.12.2",
    "dotenv": "^17.2.2",
    "mysql2": "^3.15.0"
  },
  "devDependencies": {
    "@babel/generator": "^7.28.3",
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "jest": "^30.1.3",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2"
  }
}
